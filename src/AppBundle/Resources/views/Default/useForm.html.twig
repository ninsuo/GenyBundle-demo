{# AppBundle:forms:useForm.html.twig #}
{% extends 'FuzQuickStartBundle::layout.html.twig' %}

{% block body %}

    <div class="row">

        <div class="col-md-6">
            <div class="text-center h2">Form</div>
            {{ geny_form(id, form) }}

            {% if data|default(false) %}

                <pre>{{ data | json_encode(constant('JSON_PRETTY_PRINT')) }}</pre>

            {% endif %}


        </div>
    </div>
{% endblock %}